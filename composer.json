{
    "name" : "orkestra/daemon",
    "description" : "Run PHP scripts as a background daemon",
    "license" : "MIT",
    "require" : {
        "php" : ">=5.3.2",
        "ext-pcntl" : "*",
        "ext-posix" : "*"
    },
    "require-dev" : {
        "ext-test_helpers" : "*",
        "ext-runkit" : "*",
        "symfony/process" : "2.1.*"
    },
    "suggest" : {
        "symfony/process" : "To enable daemonizing Symfony Processes"
    },
    "autoload" : {
        "psr-0" : {
            "Orkestra\\Daemon\\Tests" : "tests/",
            "Orkestra\\Daemon" : "lib/"
        }
    }
}
